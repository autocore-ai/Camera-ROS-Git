<launch>
	<!-- declare arguments with default values -->
	<arg name="proto_file" default="$(find obj_detect)/models/MobileNetSSD_deploy.prototxt"/>
	<arg name="model_file" default="$(find obj_detect)/models/MobileNetSSD_deploy.caffemodel"/>
	<arg name="model_name" default="mssd_300"/>
    	<arg name="image_source_topic" default="/usb_cam/image_raw"/>
    	<arg name="image_object_topic" default="/image_obj"/>

	<node pkg="obj_detect" type="obj_detect" name="obj_detect" output="screen">
	<param name="proto_file" value="$(arg proto_file)"/>
	<param name="model_file" value="$(arg model_file)"/>
	<param name="model_name" value="$(arg model_name)"/>
        <param name="image_source_topic" value="$(arg image_source_topic)"/>
        <param name="image_object_topic" value="$(arg image_object_topic)"/>
	</node>
</launch>
